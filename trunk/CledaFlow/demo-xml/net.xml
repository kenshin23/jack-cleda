<!-- **************************************** -->
<!-- net-petri definition                     -->
<!-- **************************************** -->

<net-petri-def name="..." doc-type="...">

	<!-- **************************************** -->
	<!-- The list of places in the net-petri      -->
	<!-- **************************************** -->

	<place-list>
		<place name="..." />
		<place name="..." />
		...
	</place-list>

	<!-- **************************************** -->
	<!-- The list of trans-set and trans          -->
	<!-- **************************************** -->

	<trans-list>
		<trans-set name="...">

			<!-- **************************************** -->
			<!-- Opt 1 : Allowed Opt1 or Opt2             -->
			<!-- **************************************** -->

			<agent-def time="..." class="..." method="..." />

			<!-- **************************************** -->
			<!-- Opt 2 : Allowed Opt1 or Opt2             -->
			<!-- **************************************** -->

			<privilege name="..." />
			<work-list name="..." />

			<!-- **************************************** -->
			<!-- Meta Data                                -->
			<!-- **************************************** -->

			<meta-data key="..." val="..." />
			<meta-data key="..." val="..." />
			...

			<!-- **************************************** -->
			<!-- Doc Section State                        -->
			<!-- **************************************** -->

			<doc-section-state name="..." state="..." />
			<doc-section-state name="..." state="..." />
			...

			<!-- **************************************** -->
			<!-- Trans List                               -->
			<!-- **************************************** -->

			<trans name="..." type="...">
				<meta-data key="..." val="..." />
				<meta-data key="..." val="..." />
				...

				<pre-place name="..." />
				<pre-place name="..." />
				...

				<pos-place name="..." />
				<pos-place name="..." />
				...
			</trans>
			...
		</trans-set>
		...
	</trans-list>

	<!-- **************************************** -->
	<!-- The list of states in the net-petri      -->
	<!-- **************************************** -->

	<state-list>
		<state-grp name="..." terminal="...">

			<!-- **************************************** -->
			<!-- Meta Data                                -->
			<!-- **************************************** -->

			<meta-data key="..." val="..." />
			<meta-data key="..." val="..." />
			...

			<!-- **************************************** -->
			<!-- Doc Section State                        -->
			<!-- **************************************** -->

			<doc-section-state name="..." state="..." />
			<doc-section-state name="..." state="..." />
			...

			<!-- **************************************** -->
			<!-- State Set List                           -->
			<!-- **************************************** -->

			<state-set name="...">
				<place name="..." tokens="..." />
				<place name="..." tokens="..." />
				...
			</state-set>
			...
		</state-grp>
		...
	</state-list>

</net-petri-def>
