<!-- **************************************** -->
<!-- net-petri definition                     -->
<!-- **************************************** -->

<net-petri-def name="test-net" doc-type="test-doc">

    <!-- **************************************** -->
    <!-- The global MetaData                      -->
    <!-- **************************************** -->

	<meta-data key="java.echo.doc.editor"     val="com.minotauro.memo.doc.FrmUserRegistration"  />
	<meta-data key="java.echo.doc.i18n.class" val="com.minotauro.memo.doc.UserRegistrationI18N" />

	<!-- **************************************** -->
	<!-- The role list and delegators             -->
	<!-- **************************************** -->

	<roles-list>
		<role name="init" delegator="com.minotauro.workflow.impl.WorkflowDelegatorImpl"/>
	</roles-list>

	<!-- **************************************** -->
	<!-- The list of places in the net-petri      -->
	<!-- **************************************** -->

	<place-list>
		<place name="p0" />
		<place name="p1" />
		<place name="p2" />
		<place name="p3" />
		<place name="p4" />
		<place name="p5" />
	</place-list>

	<!-- **************************************** -->
	<!-- The list of trans-set and trans          -->
	<!-- **************************************** -->

	<trans-list>
		<trans-set name="ts1">
			<work name="worklist" />
			<role name="init"     />

			<trans name="t1">
				<pre-place name="p0" />
				<pos-place name="p1" />
			</trans>
		</trans-set>

		<trans-set name="ts2">
			<work name="worklist" />
			<role name="init"     />

			<trans name="t2">
				<pre-place name="p1" />
				<pos-place name="p2" />
			</trans>

			<trans name="t3">
				<pre-place name="p1" />
				<pos-place name="p3" />
			</trans>
		</trans-set>

		<trans-set name="ts3">
			<work name="worklist" />
			<role name="init"     />

			<trans name="t4">
				<pre-place name="p2" />
				<pos-place name="p4" />
			</trans>
		</trans-set>

		<trans-set name="ts4">
			<work name="worklist" />
			<role name="init"     />

			<trans name="t5">
				<pre-place name="p2" />
				<pos-place name="p5" />
			</trans>
		</trans-set>

		<trans-set name="ts5">
			<work name="worklist" />
			<role name="init"     />

			<trans name="t6">
				<pre-place name="p4" />
				<pos-place name="p1" />
			</trans>
		</trans-set>

		<trans-set name="ts6">
			<work name="worklist" />
			<role name="init"     />

			<trans name="t7">
				<pre-place name="p5" />
				<pos-place name="p1" />
			</trans>
		</trans-set>
	</trans-list>

	<!-- **************************************** -->
	<!-- The list of states in the net-petri      -->
	<!-- **************************************** -->

	<state-list>
		<state-grp name="sg1" terminal="false">
			<state-set name="ss1">
				<place name="p0" tokens="1" />
			</state-set>
		</state-grp>
		<state-grp name="sg2" terminal="false">
			<state-set name="ss2">
				<place name="p1" tokens="1" />
			</state-set>
		</state-grp>
		<state-grp name="sg3" terminal="false">
			<state-set name="ss3">
				<place name="p2" tokens="1" />
			</state-set>
		</state-grp>
		<state-grp name="sg4" terminal="false">
			<state-set name="ss4">
				<place name="p3" tokens="1" />
			</state-set>
		</state-grp>
		<state-grp name="sg5" terminal="false">
			<state-set name="ss5">
				<place name="p4" tokens="1" />
			</state-set>
		</state-grp>
		<state-grp name="sg6" terminal="false">
			<state-set name="ss6">
				<place name="p5" tokens="1" />
			</state-set>
		</state-grp>
	</state-list>

</net-petri-def>
